{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "rawApexGamesApp": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "changeDetection": "OnPush",
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "projects/app",
            "sourceRoot": "projects/app/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "ngx-build-plus:browser",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "zone.js/dist/zone-error",
                            "keycode"
                        ],
                        "outputPath": "dist/rawApexGamesApp_dev",
                        "index": "projects/app/src/index.html",
                        "main": "projects/app/src/main.ts",
                        "polyfills": "projects/app/src/polyfills.ts",
                        "tsConfig": "projects/app/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/app/src/overwolf/dev/",
                                "output": "/"
                            },
                            {
                                "glob": "config.json",
                                "input": "projects/app/src/configs/",
                                "output": "/"
                            },
                            "projects/app/src/assets/images",
                            "projects/app/src/assets/svgs",
                            "projects/app/src/assets/fonts"
                        ],
                        "optimization": true,
                        "sourceMap": true,
                        "namedChunks": true,
                        "vendorChunk": true,
                        "buildOptimizer": false,
                        "styles": [
                            "projects/app/src/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "projects/app/src/assets/styles/",
                                "./node_modules"
                            ]
                        },
                        "scripts": [
                            "projects/app/src/ga.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "outputPath": "dist/rawApexGamesApp",
                            "fileReplacements": [
                                {
                                    "replace": "projects/app/src/environments/environment.ts",
                                    "with": "projects/app/src/environments/environment.prod.ts"
                                },
                                {
                                    "replace": "projects/app/src/configs/config.json",
                                    "with": "projects/app/src/configs/config.prod.json"
                                }
                            ],
                            "assets": [
                                {
                                    "glob": "**/*",
                                    "input": "projects/app/src/overwolf/prod/",
                                    "output": "/"
                                },
                                "projects/app/src/assets/images",
                                "projects/app/src/assets/svgs",
                                "projects/app/src/assets/fonts"
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": false,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "3mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                }
                            ]
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/app/test.ts",
                        "polyfills": "projects/app/src/polyfills.ts",
                        "tsConfig": "projects/app/tsconfig.spec.json",
                        "karmaConfig": "projects/app/karma.conf.js",
                        "assets": [
                            "projects/app/src/assets/images",
                            "projects/app/src/assets/svgs",
                            "projects/app/src/assets/fonts"
                        ],
                        "styles": [
                            "projects/app/src/styles.scss"
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "projects/app/src/assets/styles/",
                                "./node_modules"
                            ]
                        },
                        "scripts": [],
                        "codeCoverage": true
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/app/tsconfig.app.json",
                            "projects/app/tsconfig.spec.json",
                            "projects/app/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/app/e2e/protractor.conf.js",
                        "devServerTarget": "rawApexGamesApp:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "rawApexGamesApp:serve:production"
                        }
                    }
                }
            }
        },
        "rawApexGamesWeb": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "projects/web",
            "sourceRoot": "projects/web/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/rawApexGamesWeb_dev",
                        "index": "projects/web/src/index.html",
                        "main": "projects/web/src/main.ts",
                        "polyfills": "projects/web/src/polyfills.ts",
                        "tsConfig": "projects/web/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "projects/web/src/assets/images",
                            "projects/web/src/assets/fonts"
                        ],
                        "styles": [
                            "projects/web/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "outputPath": "dist/rawApexGamesWeb",
                            "fileReplacements": [
                                {
                                    "replace": "projects/web/src/environments/environment.ts",
                                    "with": "projects/web/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "rawApexGamesWeb:build",
                        "open": true,
                        "liveReload": true
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "rawApexGamesWeb:build:production"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/web/test.ts",
                        "polyfills": "projects/web/src/polyfills.ts",
                        "tsConfig": "projects/web/tsconfig.spec.json",
                        "karmaConfig": "projects/web/karma.conf.js",
                        "assets": [
                            "projects/web/src/assets/fonts"
                        ],
                        "styles": [
                            "projects/web/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/web/tsconfig.app.json",
                            "projects/web/tsconfig.spec.json",
                            "projects/web/e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/web/e2e/protractor.conf.js",
                        "devServerTarget": "rawApexGamesWeb:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "rawApexGamesWeb:serve:production"
                        }
                    }
                }
            }
        }
    },
    "schematics": {
        "@schematics/angular:component": {
            "style": "scss"
        }
    }
}