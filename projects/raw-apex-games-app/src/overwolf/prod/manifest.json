{
    "$schema": "https://raw.githubusercontent.com/overwolf/community-gists/master/overwolf-manifest-schema.json",
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "Raw Apex Games",
        "author": "MasterKriff",
        "version": "0.13.1",
        "minimum-overwolf-version": "0.165.0",
        "minimum-gep-version": "0.165.0",
        "description": "Raw Apex Games",
        "dock_button_title": "Raw Apex Games",
        "icon": "IconMouseOver.png",
        "icon_gray": "IconMouseNormal.png",
        "launcher_icon": "LauncherIcon.ico",
        "window_icon": "TaskbarIcon.png"
    },
    "permissions": [
        "Hotkeys",
        "Extensions",
        "Clipboard",
        "Profile",
        "FileSystem",
        "GameInfo",
        "Streaming",
        "Media",
        "DesktopStreaming",
        "VideoCaptureSettings",
        "Tray"
    ],
    "data": {
        "start_window": "background",
        "windows": {
            "background": {
                "file": "index.html",
                "is_background_page": true
            },
            "development-tools": {
                "file": "index.html",
                "desktop_only": true,
                "native_window": true,
                "resizable": true,
                "transparent": true,
                "show_in_taskbar": true,
                "allow_local_file_access": true,
                "size": {
                    "width": 1600,
                    "height": 900
                },
                "min_size": {
                    "width": 300,
                    "height": 250
                }
            },
            "main-desktop": {
                "file": "index.html",
                "desktop_only": true,
                "native_window": true,
                "use_os_windowing": true,
                "in_game_only": false,
                "transparent": true,
                "resizable": true,
                "show_in_taskbar": true,
                "restrict_to_game_bounds": false,
                "keep_window_location": true,
                "allow_local_file_access": true,
                "block_top_window_navigation": true,
                "popup_blocker": true,
                "size": {
                    "width": 850,
                    "height": 675
                },
                "min_size": {
                    "width": 750,
                    "height": 600
                }
            },
            "main-ingame": {
                "file": "index.html",
                "desktop_only": false,
                "native_window": false,
                "use_os_windowing": false,
                "in_game_only": true,
                "transparent": true,
                "resizable": false,
                "show_in_taskbar": false,
                "restrict_to_game_bounds": true,
                "keep_window_location": false,
                "allow_local_file_access": true,
                "block_top_window_navigation": true,
                "focus_game_takeover": "ReleaseOnHidden",
                "focus_game_takeover_release_hotkey": "toggle_main_ingame",
                "topmost": true,
                "popup_blocker": true,
                "size": {
                    "width": 850,
                    "height": 675
                },
                "min_size": {
                    "width": 850,
                    "height": 675
                },
                "max_size": {
                    "width": 850,
                    "height": 675
                }
            },
            "lobby-status": {
                "file": "index.html",
                "in_game_only": true,
                "resizable": false,
                "transparent": true,
                "show_in_taskbar": false,
                "clickthrough": true,
                "mute": true,
                "popup_blocker": true,
                "allow_local_file_access": true,
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            "hud-match-timer": {
                "file": "index.html",
                "in_game_only": true,
                "resizable": false,
                "transparent": true,
                "show_in_taskbar": false,
                "clickthrough": true,
                "mute": true,
                "popup_blocker": true,
                "allow_local_file_access": true,
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            "hud-mini-inventory": {
                "file": "index.html",
                "in_game_only": true,
                "resizable": false,
                "transparent": true,
                "show_in_taskbar": false,
                "clickthrough": true,
                "mute": true,
                "popup_blocker": true,
                "allow_local_file_access": true,
                "size": {
                    "width": 1,
                    "height": 1
                }
            },
            "match-summary": {
                "file": "index.html",
                "desktop_only": false,
                "native_window": false,
                "use_os_windowing": false,
                "in_game_only": true,
                "transparent": true,
                "resizable": false,
                "show_in_taskbar": false,
                "restrict_to_game_bounds": true,
                "keep_window_location": false,
                "allow_local_file_access": true,
                "block_top_window_navigation": true,
                "size": {
                    "width": 1,
                    "height": 1
                }
            }
        },
        "game_targeting": {
            "type": "dedicated",
            "game_ids": [
                21566
            ]
        },
        "game_events": [
            21566
        ],
        "launch_events": [
            {
                "event": "GameLaunch",
                "tracked": true,
                "event_data": {
                    "game_ids": [
                        21566
                    ]
                },
                "start_minimized": true
            }
        ],
        "hotkeys": {
            "toggle_main_ingame": {
                "title": "Show/Hide In-Game Window",
                "action-type": "custom",
                "default": "Alt+F",
                "passthrough": false
            }
        },
        "protocol_override_domains": {
            "googlesyndication": "http"
        },
        "externally_connectable": {
            "matches": [
                "https://*.allfather.app",
                "https://*.rawapexgames.com/TODO",
                "https://discord.gg/v5cfBTq",
                "https://*.mozambiquehe.re",
                "https://*.overwolf.com",
                "https://overwolf.github.io",
                "https://*.google.com",
                "https://*.google-analytics.com",
                "https://*.googletagmanager.com",
                "https://*.googleapis.com",
                "https://*.youtube.com"
            ]
        },
        "force_browser": "user",
        "disable_dt": false,
        "disable_cleanup": true,
        "developer": {
            "enable_auto_refresh": false,
            "reload_delay": 1000,
            "filter": "*.*"
        }
    }
}