{
    "$schema": "./config.schema.json",
    "assumptions": {
        "opponentShieldDefault": 75,
        "opponentHealthDefault": 100,
        "isRosterNullPlayerDisconnect": true,
        "knockdownExpireTime": 10000,
        "eliminationExpireTime": 300000
    },
    "facts": {
        "respawnBannerExpireTime": 60000,
        "maxShield": 125,
        "maxHealth": 100,
        "maxUltimateCooldownTime": 360000,
        "maxSquadSize": 3
    },
    "featureFlags": {
        "enableMatchTimerWindow": true,
        "enableUltTimerWindow": true,
        "enableInflictionInsightWindow": true,
        "inflictionInsight": {
            "showAssumedOpponentTeammateShields": false,
            "showAssumedOpponentTeammateHealth": false,
            "assumeKnockdownExpires": true,
            "assumeEliminationExpires": true
        },
        "legendSelectAssist": {
            "legendStats": true,
            "complimentaryLegends": true
        }
    },
    "featureConfigs": {
        "inflictionInsight": {
            "refreshTime": 1000,
            "damageResetTime": 20000
        },
        "legendSelectAssist": {
            "minLegendStatsMatches": 5,
            "maxComplimentaryLegends": 3,
            "minShowComplimentaryLegendsMatches": 10,
            "limitComplimentaryLegendsMatches": 100,
            "complimentaryLegendsWeights": {
                "winWeight": 0.25,
                "placementWeight": 0.25,
                "damageWeight": 0.25,
                "eliminationWeight": 0.25,
                "durationWeight": 0
            }
        },
        "ultTimer": {
            "refreshTime": 1000,
            "maxHistoryCount": 5,
            "highConfidenceAmount": 0.7,
            "lowConfidenceAmount": 0.35
        }
    },
    "general": {
        "playerDeathHUDTimeout": 30000,
        "matchEndHUDTimeout": 15000
    },
    "overwolfQuirks": {},
    "uiContainers": {
        "inflictionInsight": {
            "defaultPosition": {
                "x": 0.5,
                "y": 0.95
            },
            "positionUnit": "percent",
            "positionXAnchor": "left",
            "positionYAnchor": "bottom"
        }
    }
}
