{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "assumptions": {
            "type": "object",
            "properties": {
                "opponentShieldDefault": {
                    "type": "integer"
                },
                "opponentHealthDefault": {
                    "type": "integer"
                },
                "isRosterNullPlayerDisconnect": {
                    "type": "boolean"
                },
                "knockdownExpireTime": {
                    "type": "number"
                },
                "eliminationExpireTime": {
                    "type": "number"
                }
            },
            "required": [
                "opponentShieldDefault",
                "opponentHealthDefault",
                "isRosterNullPlayerDisconnect",
                "knockdownExpireTime",
                "eliminationExpireTime"
            ]
        },
        "facts": {
            "type": "object",
            "properties": {
                "respawnBannerExpireTime": {
                    "type": "integer"
                },
                "maxShield": {
                    "type": "integer"
                },
                "maxHealth": {
                    "type": "integer"
                },
                "maxUltimateCooldownTime": {
                    "type": "integer"
                },
                "maxSquadSize": {
                    "type": "integer"
                }
            },
            "required": ["maxShield", "maxHealth", "maxUltimateCooldownTime", "maxSquadSize"]
        },
        "featureFlags": {
            "type": "object",
            "properties": {
                "enableMatchTimerWindow": {
                    "type": "boolean"
                },
                "enableUltTimerWindow": {
                    "type": "boolean"
                },
                "enableInflictionInsightWindow": {
                    "type": "boolean"
                },
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "showAssumedOpponentTeammateShields": {
                            "type": "boolean"
                        },
                        "showAssumedOpponentTeammateHealth": {
                            "type": "boolean"
                        },
                        "assumeKnockdownExpires": {
                            "type": "boolean"
                        },
                        "assumeEliminationExpires": {
                            "type": "boolean"
                        }
                    },
                    "required": [
                        "showAssumedOpponentTeammateShields",
                        "showAssumedOpponentTeammateHealth",
                        "assumeKnockdownExpires",
                        "assumeEliminationExpires"
                    ]
                },
                "legendSelectAssist": {
                    "type": "object",
                    "properties": {
                        "legendStats": {
                            "type": "boolean"
                        },
                        "complimentaryLegends": {
                            "type": "boolean"
                        }
                    },
                    "required": ["legendStats", "complimentaryLegends"]
                }
            },
            "required": [
                "enableMatchTimerWindow",
                "enableUltTimerWindow",
                "enableInflictionInsightWindow",
                "inflictionInsight",
                "legendSelectAssist"
            ]
        },
        "featureConfigs": {
            "type": "object",
            "properties": {
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "damageResetTime": {
                            "type": "number"
                        },
                        "refreshTime": {
                            "type": "number"
                        }
                    },
                    "required": ["damageResetTime", "refreshTime"]
                },
                "legendSelectAssist": {
                    "type": "object",
                    "properties": {
                        "maxComplimentaryLegends": {
                            "type": "number"
                        },
                        "minShowComplimentaryLegendsMatches": {
                            "type": "number"
                        },
                        "limitLegendStatsMatches": {
                            "type": "number"
                        },
                        "limitComplimentaryLegendsMatches": {
                            "type": "number"
                        },
                        "minLegendStatsMatches": {
                            "type": "number"
                        },
                        "complimentaryLegendsWeights": {
                            "type": "object",
                            "properties": {
                                "winWeight": {
                                    "type": "number"
                                },
                                "placementWeight": {
                                    "type": "number"
                                },
                                "damageWeight": {
                                    "type": "number"
                                },
                                "eliminationWeight": {
                                    "type": "number"
                                },
                                "durationWeight": {
                                    "type": "number"
                                }
                            },
                            "required": ["winWeight", "placementWeight", "damageWeight", "eliminationWeight", "durationWeight"]
                        },
                        "legendRows": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "top": {
                                            "type": "integer"
                                        },
                                        "right": {
                                            "type": "integer"
                                        },
                                        "legendIds": {
                                            "type": "array",
                                            "items": [
                                                {
                                                    "type": "string"
                                                }
                                            ]
                                        }
                                    },
                                    "required": ["top", "right", "legendIds"]
                                }
                            ]
                        }
                    },
                    "required": [
                        "maxComplimentaryLegends",
                        "minShowComplimentaryLegendsMatches",
                        "limitLegendStatsMatches",
                        "limitComplimentaryLegendsMatches",
                        "minLegendStatsMatches",
                        "complimentaryLegendsWeights",
                        "legendRows"
                    ]
                },
                "ultTimer": {
                    "type": "object",
                    "properties": {
                        "refreshTime": {
                            "type": "number"
                        },
                        "maxHistoryCount": {
                            "type": "number"
                        },
                        "highConfidenceAmount": {
                            "type": "number"
                        },
                        "lowConfidenceAmount": {
                            "type": "number"
                        }
                    },
                    "required": ["refreshTime", "maxHistoryCount"]
                }
            },
            "required": ["inflictionInsight", "ultTimer"]
        },
        "general": {
            "type": "object",
            "properties": {
                "playerDeathHUDTimeout": {
                    "type": "integer"
                },
                "matchEndHUDTimeout": {
                    "type": "integer"
                }
            },
            "required": ["playerDeathHUDTimeout", "matchEndHUDTimeout"]
        },
        "overwolfQuirks": {
            "type": "object"
        },
        "uiContainers": {
            "type": "object",
            "properties": {
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "defaultPosition": {
                            "type": "object",
                            "properties": {
                                "x": {
                                    "type": "number"
                                },
                                "y": {
                                    "type": "number"
                                }
                            }
                        },
                        "positionUnit": {
                            "type": "string"
                        },
                        "positionXAnchor": {
                            "type": "string"
                        },
                        "positionYAnchor": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "required": ["featureFlags", "assumptions", "facts", "overwolfQuirks"],
    "additionalProperties": false
}
