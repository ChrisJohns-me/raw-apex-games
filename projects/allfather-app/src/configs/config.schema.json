{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "assumptions": {
            "type": "object",
            "properties": {
                "opponentShieldDefault": {
                    "type": "integer"
                },
                "opponentHealthDefault": {
                    "type": "integer"
                },
                "isRosterNullPlayerDisconnect": {
                    "type": "boolean"
                },
                "knockdownExpireTime": {
                    "type": "number"
                },
                "eliminationExpireTime": {
                    "type": "number"
                }
            },
            "required": [
                "opponentShieldDefault",
                "opponentHealthDefault",
                "isRosterNullPlayerDisconnect",
                "knockdownExpireTime",
                "eliminationExpireTime"
            ]
        },
        "common": {
            "type": "object",
            "properties": {
                "playerDeathHUDTimeout": {
                    "type": "integer"
                },
                "matchEndHUDTimeout": {
                    "type": "integer"
                }
            },
            "required": ["playerDeathHUDTimeout", "matchEndHUDTimeout"]
        },
        "brFacts": {
            "type": "object",
            "properties": {
                "respawnBannerExpireTime": {
                    "type": "integer"
                },
                "maxUltimateCooldownTime": {
                    "type": "integer"
                },
                "maxSquadSize": {
                    "type": "integer"
                },
                "ringDamageTickRate": {
                    "type": "integer"
                },
                "rings": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "ringNumber": {
                                    "type": "number"
                                },
                                "holdStartTime": {
                                    "type": "number"
                                },
                                "holdEndTime": {
                                    "type": "number"
                                },
                                "shrinkStartTime": {
                                    "type": "number"
                                },
                                "shrinkEndTime": {
                                    "type": "number"
                                },
                                "damagePerTick": {
                                    "type": "number"
                                }
                            },
                            "required": ["ringNumber", "holdStartTime", "holdEndTime", "shrinkStartTime", "shrinkEndTime", "damagePerTick"]
                        }
                    ]
                }
            },
            "required": ["respawnBannerExpireTime", "maxUltimateCooldownTime", "maxSquadSize", "ringDamageTickRate", "rings"]
        },
        "facts": {
            "type": "object",
            "properties": {
                "maxShield": {
                    "type": "integer"
                },
                "maxHealth": {
                    "type": "integer"
                }
            },
            "required": ["maxShield", "maxHealth"]
        },
        "featureFlags": {
            "type": "object",
            "properties": {
                "enableMatchTimerWindow": {
                    "type": "boolean"
                },
                "enableLegendSelectAssistWindow": {
                    "type": "boolean"
                },
                "enableMiniInventoryWindow": {
                    "type": "boolean"
                },
                "enableUltTimerWindow": {
                    "type": "boolean"
                },
                "enableInflictionInsightWindow": {
                    "type": "boolean"
                },
                "enableHealingHelperWindow": {
                    "type": "boolean"
                },
                "enableReticleHelperWindow": {
                    "type": "boolean"
                },
                "enableVideoCapture": {
                    "type": "boolean"
                },
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "teamIndicatorUI": {
                            "type": "boolean"
                        },
                        "visualizeDamageUI": {
                            "type": "boolean"
                        },
                        "totalDamageAmount": {
                            "type": "boolean"
                        },
                        "shieldDamageAmount": {
                            "type": "boolean"
                        },
                        "healthDamageAmount": {
                            "type": "boolean"
                        },
                        "platformUI": {
                            "type": "boolean"
                        },
                        "showAssumedOpponentTeammateShields": {
                            "type": "boolean"
                        },
                        "showAssumedOpponentTeammateHealth": {
                            "type": "boolean"
                        },
                        "assumeKnockdownExpires": {
                            "type": "boolean"
                        },
                        "assumeEliminationExpires": {
                            "type": "boolean"
                        }
                    },
                    "required": [
                        "teamIndicatorUI",
                        "visualizeDamageUI",
                        "totalDamageAmount",
                        "shieldDamageAmount",
                        "healthDamageAmount",
                        "platformUI",
                        "showAssumedOpponentTeammateShields",
                        "showAssumedOpponentTeammateHealth",
                        "assumeKnockdownExpires",
                        "assumeEliminationExpires"
                    ]
                },
                "legendSelectAssist": {
                    "type": "object",
                    "properties": {
                        "legendStats": {
                            "type": "boolean"
                        },
                        "complimentaryLegends": {
                            "type": "boolean"
                        }
                    },
                    "required": ["legendStats", "complimentaryLegends"]
                },
                "reticleHelper": {
                    "type": "object",
                    "properties": {
                        "aimingReticle": {
                            "type": "boolean"
                        }
                    },
                    "required": ["aimingReticle"]
                }
            },
            "required": [
                "enableMatchTimerWindow",
                "enableMiniInventoryWindow",
                "enableUltTimerWindow",
                "enableInflictionInsightWindow",
                "enableReticleHelperWindow",
                "capture",
                "inflictionInsight",
                "legendSelectAssist",
                "reticleHelper"
            ]
        },
        "featureConfigs": {
            "type": "object",
            "properties": {
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "damageResetTime": {
                            "type": "number"
                        }
                    },
                    "required": ["damageResetTime"]
                },
                "legendSelectAssist": {
                    "type": "object",
                    "properties": {
                        "maxComplimentaryLegends": {
                            "type": "number"
                        },
                        "minShowComplimentaryLegendsMatches": {
                            "type": "number"
                        },
                        "limitLegendStatsMatches": {
                            "type": "number"
                        },
                        "limitComplimentaryLegendsMatches": {
                            "type": "number"
                        },
                        "minLegendStatsMatches": {
                            "type": "number"
                        },
                        "complimentaryLegendsWeights": {
                            "type": "object",
                            "properties": {
                                "winWeight": {
                                    "type": "number"
                                },
                                "placementWeight": {
                                    "type": "number"
                                },
                                "damageWeight": {
                                    "type": "number"
                                },
                                "eliminationWeight": {
                                    "type": "number"
                                },
                                "deathWeight": {
                                    "type": "number"
                                },
                                "knockdownWeight": {
                                    "type": "number"
                                },
                                "durationWeight": {
                                    "type": "number"
                                }
                            },
                            "required": [
                                "winWeight",
                                "placementWeight",
                                "damageWeight",
                                "eliminationWeight",
                                "assistWeight",
                                "knockdownWeight",
                                "durationWeight"
                            ]
                        },
                        "legendRows": {
                            "type": "array",
                            "items": [
                                {
                                    "type": "object",
                                    "properties": {
                                        "top": {
                                            "type": "number"
                                        },
                                        "right": {
                                            "type": "number"
                                        },
                                        "legendIds": {
                                            "type": "array",
                                            "items": [
                                                {
                                                    "type": "string"
                                                }
                                            ]
                                        }
                                    },
                                    "required": ["top", "right", "legendIds"]
                                }
                            ]
                        }
                    },
                    "required": [
                        "maxComplimentaryLegends",
                        "minShowComplimentaryLegendsMatches",
                        "limitLegendStatsMatches",
                        "limitComplimentaryLegendsMatches",
                        "minLegendStatsMatches",
                        "complimentaryLegendsWeights",
                        "legendRows"
                    ]
                },
                "ultTimer": {
                    "type": "object",
                    "properties": {
                        "maxHistoryCount": {
                            "type": "number"
                        },
                        "highConfidenceAmount": {
                            "type": "number"
                        },
                        "lowConfidenceAmount": {
                            "type": "number"
                        }
                    },
                    "required": ["maxHistoryCount", "highConfidenceAmount", "lowConfidenceAmount"]
                }
            },
            "required": ["inflictionInsight", "ultTimer"]
        },
        "general": {
            "type": "object",
            "properties": {},
            "required": []
        },
        "overwolfQuirks": {
            "type": "object"
        }
    },
    "required": ["assumptions", "common", "featureFlags", "facts", "general", "overwolfQuirks"],
    "additionalProperties": false
}
