{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "assumptions": {
            "type": "object",
            "properties": {
                "opponentShieldDefault": {
                    "type": "integer"
                },
                "opponentHealthDefault": {
                    "type": "integer"
                },
                "isRosterNullPlayerDisconnect": {
                    "type": "boolean"
                },
                "knockdownExpireTime": {
                    "type": "number"
                },
                "eliminationExpireTime": {
                    "type": "number"
                }
            },
            "required": [
                "opponentShieldDefault",
                "opponentHealthDefault",
                "isRosterNullPlayerDisconnect",
                "knockdownExpireTime",
                "eliminationExpireTime"
            ]
        },
        "facts": {
            "type": "object",
            "properties": {
                "respawnBannerExpireTime": {
                    "type": "integer"
                },
                "shieldMax": {
                    "type": "integer"
                },
                "healthMax": {
                    "type": "integer"
                }
            },
            "required": ["shieldMax", "healthMax"]
        },
        "featureFlags": {
            "type": "object",
            "properties": {
                "enableMatchTimerWindow": {
                    "type": "boolean"
                },
                "enableUltTimerWindow": {
                    "type": "boolean"
                },
                "enableInflictionInsightWindow": {
                    "type": "boolean"
                },
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "showAssumedOpponentTeammateShields": {
                            "type": "boolean"
                        },
                        "showAssumedOpponentTeammateHealth": {
                            "type": "boolean"
                        },
                        "assumeKnockdownExpires": {
                            "type": "boolean"
                        },
                        "assumeEliminationExpires": {
                            "type": "boolean"
                        }
                    },
                    "required": [
                        "showAssumedOpponentTeammateShields",
                        "showAssumedOpponentTeammateHealth",
                        "assumeKnockdownExpires",
                        "assumeEliminationExpires"
                    ]
                }
            },
            "required": ["enableMatchTimerWindow", "enableUltTimerWindow", "enableInflictionInsightWindow", "inflictionInsight"]
        },
        "featureConfigs": {
            "type": "object",
            "properties": {
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "damageResetTime": {
                            "type": "number"
                        },
                        "refreshTime": {
                            "type": "number"
                        }
                    },
                    "required": ["damageResetTime", "refreshTime"]
                },
                "ultTimer": {
                    "type": "object",
                    "properties": {
                        "refreshTime": {
                            "type": "number"
                        },
                        "maxHistoryCount": {
                            "type": "number"
                        }
                    },
                    "required": ["refreshTime", "maxHistoryCount"]
                }
            },
            "required": ["inflictionInsight", "ultTimer"]
        },
        "general": {
            "type": "object",
            "properties": {
                "playerDeathHUDTimeout": {
                    "type": "integer"
                },
                "matchEndHUDTimeout": {
                    "type": "integer"
                }
            },
            "required": ["playerDeathHUDTimeout", "matchEndHUDTimeout"]
        },
        "overwolfQuirks": {
            "type": "object"
        },
        "uiContainers": {
            "type": "object",
            "properties": {
                "inflictionInsight": {
                    "type": "object",
                    "properties": {
                        "defaultPosition": {
                            "type": "object",
                            "properties": {
                                "x": {
                                    "type": "number"
                                },
                                "y": {
                                    "type": "number"
                                }
                            }
                        },
                        "positionUnit": {
                            "type": "string"
                        },
                        "positionXAnchor": {
                            "type": "string"
                        },
                        "positionYAnchor": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "required": ["featureFlags", "assumptions", "facts", "overwolfQuirks"],
    "additionalProperties": false
}
