{
    "name": "allfather",
    "version": "0.1.0",
    "engines": {
        "node": ">= 14.0.0"
    },
    "scripts": {
        "preinstall": "npm i -g firebase-tools @angular/cli",
        "test": "npm run afapp:test -- --no-watch && npm run afweb:test -- --no-watch && npm run common:test -- --single-run",
        "common:test": "karma start ./common/karma.conf.js",
        "afapp:test": "ng test --project=allfatherApp",
        "afapp:e2e": "ng e2e --project=allfatherApp",
        "afapp:serve-debug": "ng build --project=allfatherApp --watch --plugin ~./projects/allfather-app/webpack.partial.dev.js --optimization false",
        "afapp:serve-dev": "ng build --project=allfatherApp --watch --plugin ~./projects/allfather-app/webpack.partial.dev.js",
        "afapp:build-prod": "ng build --project=allfatherApp --configuration production",
        "afapp:compose-opk": "",
        "afapp:stats": "ng build --project=allfatherApp --stats-json",
        "afapp:analyze-dev": "webpack-bundle-analyzer dist/allfatherApp_dev/stats.json",
        "affunc:serve-dev": "cd projects/allfather-func/functions && npm run serve",
        "affunc:deploy-dev": "",
        "affunc:deploy-prod": "",
        "afweb:test": "ng test --project=allfatherWeb",
        "afweb:e2e": "ng e2e --project=allfatherWeb",
        "afweb:serve-dev": "ng build --watch --project=allfatherWeb",
        "afweb:build-prod": "ng build --project=allfatherWeb --configuration production"
    },
    "browserslist": [
        "chrome >= 83"
    ],
    "private": true,
    "dependencies": {
        "@angular/animations": "^13.3.1",
        "@angular/common": "^13.3.1",
        "@angular/compiler": "^13.3.1",
        "@angular/core": "^13.3.1",
        "@angular/forms": "^13.3.1",
        "@angular/platform-browser": "^13.3.1",
        "@angular/platform-browser-dynamic": "^13.3.1",
        "@angular/router": "^13.3.1",
        "@mdi/js": "^6.6.96",
        "@overwolf/types": "^3.17.0",
        "@popperjs/core": "^2.11.5",
        "@svgdotjs/svg.js": "^3.1.2",
        "bootstrap": "^5.1.3",
        "bootswatch": "^5.1.3",
        "chart.js": "^3.7.1",
        "chartjs-adapter-date-fns": "^2.0.0",
        "chartjs-plugin-datalabels": "^2.0.0",
        "d3": "^7.4.4",
        "d3-contour": "^3.0.1",
        "d3-transition": "^3.0.1",
        "date-fns": "^2.28.0",
        "dexie": "^3.2.1",
        "dexie-export-import": "^1.0.3",
        "dexie-observable": ">=3.0.0-beta.11",
        "keycode": "^2.2.1",
        "md5": "^2.3.0",
        "ngx-build-plus": "^13.0.1",
        "ngx-date-fns": "^9.0.0",
        "rxjs": "^7.5.5",
        "seedrandom": "^3.0.5",
        "tslib": "^2.4.0",
        "uuid": "^8.3.2",
        "zone.js": "^0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.3.5",
        "@angular/cli": "^13.3.3",
        "@angular/compiler-cli": "^13.3.1",
        "@types/bootstrap": "^5.1.9",
        "@types/d3": "^7.1.0",
        "@types/jasmine": "^4.0.2",
        "@types/md5": "^2.3.2",
        "@types/node": "^17.0.29",
        "@types/resize-observer-browser": "^0.1.7",
        "@types/seedrandom": "^3.0.2",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "codelyzer": "^6.0.2",
        "concurrently": "^7.1.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "exports-loader": "^3.1.0",
        "firebase-tools": "^10.6.0",
        "jasmine-core": "^4.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.3.17",
        "karma-chrome-launcher": "^3.1.1",
        "karma-coverage": "^2.2.0",
        "karma-jasmine": "~4.0.2",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-typescript": "^5.5.3",
        "livereload": "^0.9.3",
        "prettier": "^2.6.2",
        "prettier-eslint": "^13.0.0",
        "prettier-eslint-cli": "^5.0.1",
        "protractor": "~7.0.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3",
        "webpack-bundle-analyzer": "^4.5.0"
    }
}
