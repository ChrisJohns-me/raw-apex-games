{
    "name": "raw-apex-games-app",
    "version": "0.13.1",
    "engines": {
        "node": ">= 14.0.0"
    },
    "scripts": {
        "preinstall": "npm i -g firebase-tools @angular/cli",
        "test": "npm run app:test -- --no-watch && npm run web:test -- --no-watch && npm run common:test -- --single-run",
        "common:test": "karma start ./common/karma.conf.js",
        "app:test": "ng test --project=rawApexGamesApp",
        "app:e2e": "ng e2e --project=rawApexGamesApp",
        "app:serve-dev": "ng build --project=rawApexGamesApp --watch --plugin ~./projects/app/webpack.partial.dev.js --optimization false",
        "app:build-prod": "ng build --project=rawApexGamesApp --configuration production",
        "app:compose-opk": "",
        "app:stats": "ng build --project=rawApexGamesApp --stats-json",
        "app:analyze-dev": "webpack-bundle-analyzer dist/rawApexGamesApp_dev/stats.json",
        "api:serve-db": "gcloud emulators firestore start",
        "api:serve-dev": "npx ts-node projects/api/src/index.ts",
        "api:deploy-dev": "",
        "api:deploy-prod": "",
        "web:test": "ng test --project=rawApexGamesWeb",
        "web:e2e": "ng e2e --project=rawApexGamesWeb",
        "web:serve-dev": "ng serve --project=rawApexGamesWeb",
        "web:build-prod": "ng build --project=rawApexGamesWeb --configuration production"
    },
    "browserslist": [
        "chrome >= 83"
    ],
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.3.0",
        "@angular/common": "^14.3.0",
        "@angular/compiler": "^14.3.0",
        "@angular/core": "^14.3.0",
        "@angular/forms": "^14.3.0",
        "@angular/platform-browser": "^14.3.0",
        "@angular/platform-browser-dynamic": "^14.3.0",
        "@angular/router": "^14.3.0",
        "@mdi/js": "^6.6.96",
        "@overwolf/types": "^3.23.1",
        "@popperjs/core": "^2.11.5",
        "@svgdotjs/svg.js": "^3.1.2",
        "body-parser": "^1.20.2",
        "bootstrap": "^5.1.3",
        "bootswatch": "^5.1.3",
        "chart.js": "^3.7.1",
        "chartjs-adapter-date-fns": "^2.0.0",
        "chartjs-plugin-datalabels": "^2.0.0",
        "d3": "^7.4.4",
        "d3-contour": "^3.0.1",
        "d3-transition": "^3.0.1",
        "date-fns": "^2.28.0",
        "dexie": "^3.2.1",
        "dexie-export-import": "^1.0.3",
        "dexie-observable": ">=3.0.0-beta.11",
        "express": "^4.17.1",
