{
  "name": "allfather",
  "version": "0.0.1-alpha.1",
  "engines": {
    "node": ">= 14.0.0"
  },
  "scripts": {
    "test": "npm run app:test -- --no-watch && npm run web:test -- --no-watch && npm run shared:test -- --single-run",
    "shared:test": "karma start ./shared/karma.conf.js",
    "app:test": "ng test --project=allfatherApp",
    "app:e2e": "ng e2e --project=allfatherApp",
    "app:build-debug": "ng build --project=allfatherApp --watch --plugin ~./projects/allfather-app/webpack.partial.dev.js --optimization false",
    "app:build-dev": "ng build --project=allfatherApp --watch --plugin ~./projects/allfather-app/webpack.partial.dev.js",
    "app:build-prod": "ng build --project=allfatherApp --configuration production",
    "web:test": "ng test --project=allfatherWeb",
    "web:e2e": "ng e2e --project=allfatherWeb",
    "web:build-dev": "ng build --watch --project=allfatherWeb",
    "web:build-prod": "ng build --project=allfatherWeb --configuration production"
  },
  "browserslist": [
    "chrome >= 83"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.0.2",
    "@angular/common": "^12.0.2",
    "@angular/compiler": "^12.0.2",
    "@angular/core": "^12.0.2",
    "@angular/forms": "^12.0.2",
    "@angular/platform-browser": "^12.0.2",
    "@angular/platform-browser-dynamic": "^12.0.2",
    "@angular/router": "^12.0.2",
    "@mdi/js": "^5.9.55",
    "@overwolf/types": "^3.1.0",
    "@popperjs/core": "^2.9.2",
    "@svgdotjs/svg.js": "^3.0.16",
    "bootstrap": "^5.0.1",
    "bootswatch": "^5.0.1",
    "chart.js": "^3.3.0",
    "chartjs-adapter-date-fns": "^2.0.0",
    "d3": "^6.7.0",
    "d3-contour": "^2.0.0",
    "d3-transition": "^2.0.0",
    "date-fns": "^2.22.0",
    "dexie": "^3.0.3",
    "dexie-export-import": "^1.0.0",
    "dexie-observable": ">=3.0.0-beta.10",
    "ngx-build-plus": "^12.0.1",
    "ngx-date-fns": "^8.1.0",
    "rxjs": "~6.6.0",
    "seedrandom": "^3.0.5",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.2",
    "@angular/cli": "^12.1.0",
    "@angular/compiler-cli": "^12.0.2",
    "@types/bootstrap": "^5.0.15",
    "@types/d3": "^6.7.0",
    "@types/jasmine": "^3.7.6",
    "@types/node": "^15.12.5",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/seedrandom": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.31.2",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "exports-loader": "^3.0.0",
    "jasmine-core": "^3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-typescript": "^5.5.1",
    "livereload": "^0.9.3",
    "prettier": "2.3.2",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.2.3"
  }
}
