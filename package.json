{
    "name": "raw-apex-games",
    "version": "0.13.1",
    "private": true,
    "engines": {
        "node": ">= 18.0.0"
    },
    "browserslist": [
        "chrome >= 83"
    ],
    "type": "module",
    "scripts": {
        "preinstall": "npm i -w -g firebase-tools @angular/cli",
        "db:start-dev": "firebase emulators:start --export-on-exit ./emulator-data",
        "api:compile-dev": "npx tsc -p ./projects/api/tsconfig.json -w",
        "api:server-dev": "cross-env NODE_ENV=DEV npx nodemon ./dist/api-out-tsc/projects/api/src/main.js",
        "api:start-dev": "concurrently \"npx db:start-dev\" \"npx api:compile-dev\" \"npx api:server-dev\"",
        "api:deploy-dev": "",
        "api:deploy-prod": "",
        "app:start-dev": "ng build --project=rawApexGamesApp --watch --plugin ~./projects/app/webpack.partial.dev.cjs --optimization false",
        "app:build-prod": "ng build --project=rawApexGamesApp --configuration production",
        "app:compose-opk": "",
        "app:stats": "ng build --project=rawApexGamesApp --stats-json",
        "app:analyze-dev": "webpack-bundle-analyzer dist/rawApexGamesApp_dev/stats.json",
        "web:start-dev": "ng serve --project=rawApexGamesWeb",
        "web:build-prod": "ng build --project=rawApexGamesWeb --configuration production"
    },
    "imports": {
        "#######": "### Path Import Aliases for API Project ###",
        "#api/*": "./dist/api-out-tsc/projects/api/src/*",
        "#app/*": "./dist/api-out-tsc/projects/app/src/app/*",
        "#shared/*": "./dist/api-out-tsc/projects/shared/*",
        "#web/*": "./dist/api-out-tsc/projects/web/src/app/*"
    },
    "dependencies": {
        "@angular/animations": "^15.2.7",
        "@angular/common": "^15.2.7",
        "@angular/compiler": "^15.2.7",
        "@angular/core": "^15.2.7",
        "@angular/forms": "^15.2.7",
        "@angular/platform-browser": "^15.2.7",
        "@angular/platform-browser-dynamic": "^15.2.7",
        "@angular/router": "^15.2.7",
        "@mdi/js": "^6.6.96",
        "@overwolf/types": "^3.23.1",
        "@popperjs/core": "^2.11.5",
        "@svgdotjs/svg.js": "^3.1.2",
        "body-parser": "^1.20.2",
        "bootstrap": "^5.1.3",
        "bootswatch": "^5.1.3",
        "chart.js": "^3.7.1",
        "chartjs-adapter-date-fns": "^2.0.0",
        "chartjs-plugin-datalabels": "^2.0.0",
        "cors": "^2.8.5",
        "d3": "^7.4.4",
        "d3-contour": "^3.0.1",
        "d3-transition": "^3.0.1",
        "date-fns": "^2.28.0",
        "dexie": "^3.2.1",
        "dexie-export-import": "^1.0.3",
        "dexie-observable": ">=3.0.0-beta.11",
        "express": "^4.17.1",
        "firebase": "^9.19.1",
        "firebase-admin": "^11.6.0",
        "keycode": "^2.2.1",
        "md5": "^2.3.0",
        "ngx-build-plus": "^15.0.0",
        "rxjs": "^7.5.5",
        "seedrandom": "^3.0.5",
        "socket.io": "^4.6.1",
        "socket.io-client": "^4.6.1",
        "ts-enum-util": "^4.0.2",
        "tslib": "^2.4.0",
        "uuid": "^8.3.2",
        "zone.js": "^0.11.5"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.2.6",
        "@angular/cli": "^15.2.6",
        "@angular/compiler-cli": "^15.2.7",
        "@types/bootstrap": "^5.1.9",
        "@types/d3": "^7.1.0",
        "@types/express": "^4.17.17",
        "@types/jasmine": "^4.0.2",
        "@types/md5": "^2.3.2",
        "@types/node": "^18.15.11",
        "@types/resize-observer-browser": "^0.1.7",
        "@types/seedrandom": "^3.0.2",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "codelyzer": "^6.0.2",
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "exports-loader": "^3.1.0",
        "firebase-tools": "^10.6.0",
        "jasmine-core": "^4.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "karma": "^6.3.17",
        "karma-chrome-launcher": "^3.1.1",
        "karma-coverage": "^2.2.0",
        "karma-jasmine": "~4.0.2",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-typescript": "^5.5.3",
        "livereload": "^0.9.3",
        "nodemon": "^2.0.22",
        "prettier": "^2.6.2",
        "prettier-eslint": "^13.0.0",
        "prettier-eslint-cli": "^5.0.1",
        "typescript": "^4.9.5",
        "webpack-bundle-analyzer": "^4.5.0"
    }
}