{
    "name": "allfather",
    "version": "0.0.1-alpha.1",
    "scripts": {
        "test": "npm run app:test -- --watch=false && npm run web:test -- --watch=false && npm run shared:test -- --single-run",
        "shared:test": "karma start ./shared/karma.conf.js",
        "app:test": "ng test --project=allfatherApp",
        "app:e2e": "ng e2e --project=allfatherApp",
        "app:build-dev": "ng build --watch --plugin ~./projects/allfather-app/webpack.partial.dev.js --project=allfatherApp",
        "app:build-prod": "ng build --prod --project=allfatherApp",
        "web:test": "ng test --project=allfatherWeb",
        "web:e2e": "ng e2e --project=allfatherWeb",
        "web:build-dev": "ng build --watch --project=allfatherWeb",
        "web:build-prod": "ng build --prod --project=allfatherWeb"
    },
